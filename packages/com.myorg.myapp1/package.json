{
  "name": "myapp1",
  "version": "1.0.0",
  "description": "UI5 Application: myapp1",
  "author": "Volker Buzek",
  "license": "Apache-2.0",
  "scripts": {
    "build": "ui5 build --clean-dest",
    "build:opt": "ui5 build self-contained --clean-dest --all",
    "start": "ui5 serve --port 8080 -o index.html",
    "start:dist": "ui5 serve  --port 8080 -o index.html --config ui5-dist.yaml",
    "ts-typecheck": "tsc --noEmit",
    "lint": "eslint webapp",
    "test": "npm run lint",
    "wdi5": "wdio run ./wdio.conf.ts --autoCompileOpts.tsNodeOpts.project=test/tsconfig.json",
    "wdi5:bstack": "wdio run ./wdio-bstack.conf.ts --autoCompileOpts.tsNodeOpts.project=test/tsconfig.json",
    "wdi5:sl": "wdio run ./wdio-sl.conf.ts --autoCompileOpts.tsNodeOpts.project=test/tsconfig.json"
  },
  "devDependencies": {
    "@openui5/ts-types-esm": "^1.114.0",
    "@sapui5/types": "1.114.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@ui5/cli": "^3",
    "@wdio/browserstack-service": "^7",
    "@wdio/cli": "^7",
    "@wdio/local-runner": "^7",
    "@wdio/mocha-framework": "^7",
    "@wdio/sauce-service": "^7",
    "@wdio/spec-reporter": "^7",
    "chromedriver": "latest",
    "dotenv": "^16.1.3",
    "eslint": "^8.37.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "ui5-middleware-livereload": "latest",
    "ui5-middleware-simpleproxy": "latest",
    "ui5-tooling-transpile": "latest",
    "wdio-chromedriver-service": "^7",
    "wdio-ui5-service": "latest"
  }
}
